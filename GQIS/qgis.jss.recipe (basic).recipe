<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
<dict>
	<key>Description</key>
	<string>Creates installer package from downloaded QGIS dmg.</string>
	<key>Identifier</key>
	<string>com.github.smithersjr.jss.qgis</string>
	<key>Input</key>
	<dict>
		<key></key>
		<string></string>
	</dict>
	<key>MinimumVersion</key>
	<string>0.6.1</string>
    <key>ParentRecipe</key>
    <string>com.github.smithersjr.pkg.qgis</string>
	<key>Process</key>
        <array>
        <dict>
        	<key>Processor</key>
        	<string>JSSImporter</string>
        	 <key>Arguments</key>
            <dict>
				<key>pkg_path</key>
				<string>%pkg_path%</string>
				<key>prod_name</key>
				<string>%NAME%</string>
            </dict>
        </dict>
        <dict>
	<key>Processor</key>
	<string>EndOfCheckPhase</string>
        </dict>
        </array>
</dict>
</plist>